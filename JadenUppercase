String.prototype.toJadenCase = function () {
  let splitStr = this.split(" ");
  let newStr = "";

  for (let i = 0; i < splitStr.length; i++) {
    newStr += splitStr[i].substring(0, 1).toUpperCase() + splitStr[i].substring(1).toLowerCase();
    if (i < splitStr.length - 1) {
      newStr += " "
    }
  }
  
  return newStr;
};
